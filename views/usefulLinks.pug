extends layout 

block content
  include mixins/link
  .container
    .row.justify-content-center
      .col-lg-12 
        h1.text-center.mt-4= page
        hr
    .row.justify-content-center
      if links.length
        .col-lg-6
            each link in links
              +link(link.name, link.link)
      else
        h3 There are no links at the moment.